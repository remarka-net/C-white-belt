#include <math.h>
#include <iostream>
#include <vector>
using namespace std;

int main() {
	int n;
	vector<int> v;
	n = 75;
	for (int i = v.size()-1; i > -1; --i;) {
		int x;
		x = n%2;
		n = n/2;
		v.push_back(x);
		if (n==2){
			v.push_back(0);
			v.push_back(1);
			break;
		}
	}
	for (auto i : v) cout << v[i];
//	  for(int k = 0; k < v.size(); k++){
//	        cout << v[k] << ' ';
//	  }
	return 0;
}
