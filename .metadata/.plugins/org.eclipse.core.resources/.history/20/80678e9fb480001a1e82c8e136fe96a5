
#include <iostream>
#include <cmath>
using namespace std;

int main() {
	double a,b,c;
	double x1, x2;
	double D;
	//cin >> a >> b >> c;
	a=2;b=5;c=2;
	D = b*b - 4 * a * c;
	cout << D <<a <<b<<c<<endl;
	if (D < 0){
		x1 = (-b + sqrt(D))/2*a;
		x2 = (-b - sqrt(D))/2*a;
		cout<<x1<<x2;
	}else if ( D > 0){
		x1 = (-b + sqrt(D))/2*a;
		x2 = (-b - sqrt(D))/2*a;
		cout<<x1<<x2;
	}else{
		x1 = x2 = -b/2*a;
		cout<<x1<<x2;
	}
	return 0;
}
